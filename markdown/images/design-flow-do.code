function Structural Synthesis {
    Number and Type Synthesis;
    Enumeration of Contracted Graph;
    Cut-links Check;
    Planar Check;
    Isomorphism Check;
    Enumeration of Conventional Graphs;
    Degenerate Check;
    Isomorphism Check;
}

function Dimensional Synthesis {
    **Data Conversion**;
    Automatic Configuration;
    switch (Algorithms) {
        case RGA:
            RGA;
            break;
        case DE:
            DE;
            break;
        case Firefly:
            Firefly;
            break;
    }
    return **Data Conversion**;
}

function Kinematic Simulation {
    **Data Conversion**;
    Numerical Methods;
    **Data Conversion**;
}

function Backward Analysis {
    **Data Conversion**;
    Identification;
}

head:
Generalized Chain;
call Structural Synthesis;
Graph Block;
call Dimensional Synthesis;
Mechanism Expression;
call Kinematic Simulation;
call Backward Analysis;
goto head;
